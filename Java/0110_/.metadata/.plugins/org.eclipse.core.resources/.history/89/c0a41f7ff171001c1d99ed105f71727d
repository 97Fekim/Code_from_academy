import java.util.*;

public class Class_main 
{
	public static void main(String[] args) 
	{
		int[] arr = new int[10];
		
		/* 3의 배수로 배열을 채우세요, 3부터 채우세요 */
		for(int i = 0; i < 10; ++i)
			arr[i] = (i+1) * 3;
		
		/* 문제5) arr배열의 index가 3인곳의 데이터를
		  서로 교환하세요 */
		int temp;
		temp = arr[3];
		arr[3] = arr[7];
		arr[7] = temp;
		
		for(int i = 0; i < 10; ++i)
			System.out.println(arr[i]);
		
		int EMPTY = 0;
		int FULL = 1;
		
		int glass = EMPTY;
		
		if(glass == EMPTY)
			System.out.println("비어있음");
		else if(glass == FULL)
			System.out.println("차있음");
		
		
		
		
	}
}
