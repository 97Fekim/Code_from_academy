import java.util.*;

public class Class_main 
{
	public static void main(String[] args) 
	{
		int[] arr = new int[10];
		
		/* 3의 배수로 배열을 채우세요, 3부터 채우세요 */
		for(int i = 0; i < 10; ++i)
			arr[i] = (i+1) * 3;
		
		/* 문제5) arr배열의 index가 3인곳의 데이터를
		  서로 교환하세요 */
		int temp;
		temp = arr[3];
		arr[3] = arr[7];
		arr[7] = temp;
		
		for(int i = 0; i < 10; ++i)
			System.out.println(arr[i]);
		
		double didx = Math.random()*5;
		System.out.println("랜덤값 double : " + didx);
		int iidx = (int)(Math.random()*6);
		System.out.println("랜덤값 int : " + iidx);
		int result = arr[iidx];
	
	}
}
