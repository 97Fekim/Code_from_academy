import java.util.*;

public class Class_main 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("첫번째 숫자 하나를 입력하세요");
		int input1 = 0;
		input1 = sc.nextInt();	// 화면을 통해 입력 받은 숫자를 input 변수에 저장
		System.out.println("두번째 숫자 하나를 입력하세요");
		int input2 = sc.nextInt();	// 화면을 통해 입력 받은 숫자를 input 변수에 저장
		System.out.println("세번째 숫자 하나를 입력하세요");
		int input3 = sc.nextInt();	// 화면을 통해 입력 받은 숫자를 input 변수에 저장
		System.out.println("네번째 숫자 하나를 입력하세요");
		int input4 = sc.nextInt();	// 화면을 통해 입력 받은 숫자를 input 변수에 저장
		System.out.println("다섯번째 숫자 하나를 입력하세요");
		int input5 = sc.nextInt();	// 화면을 통해 입력 받은 숫자를 input 변수에 저장
		
	}
}
