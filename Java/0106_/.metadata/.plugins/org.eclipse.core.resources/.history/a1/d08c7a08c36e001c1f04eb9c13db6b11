
public class Test1 
{
	// 문제3) 함수 정의하세요 숫자 2.37f을 반환함 
	// main함수에서 위에서 정의한 함수 호출 후 main함수에서 출력하세요
	public static float func_1()
	{
		return 2.37f;
	}
	
	public static Boolean func_2()
	{
		return true;
	}
	
	public static void main(String[] args) 
	{
		float f = 0.0f;
		System.out.println(f=func_1());
		
		Boolean B;
		System.out.println(B=func_2());
	}

}
