package class_learning;

public class Car 
{
	private int doorNumber;
	private int windowNumber;
	
	private String name;
	private String color;
	
	/* 기본생성자 */
	public Car()
	{
		
	}
	
	
	public Car(int n1, int n2, String str1, String str2)
	{
		doorNumber = n1; 
		windowNumber = n2; 
		name = str1;
		color = str2;
	}
	
}
