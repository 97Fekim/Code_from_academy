package temp;

public class Class_temp 
{
	public static boolean f(int a)
	{
		if(a%2 == 0)
			return true;
		else
			return false;
	}
	
	public static void main(String[] args) 
	{
		boolean[] arr = new boolean[10];
		for(int i = 0; i < arr.length; i++)
		{
			arr[i] = (int)(Math.random()*10);
		}
	}
}
